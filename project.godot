; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

templates_search_path="res://addons/simplefsm/script_templates"

[application]

config/name="SimpleFSM"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"

[dotnet]

project/assembly_name="SimpleFSM"

[editor]

script/templates_search_path="res://addons/simplefsm/script_templates"

[editor_plugins]

enabled=PackedStringArray("res://addons/simplefsm/plugin.cfg")
